{
  "name": "web-check",
  "type": "module",
  "version": "2.0.1",
  "homepage": "https://jetestemonsite.apdp.mc",
  "scripts": {
    "start": "node server",
    "start-pm": "pm2 start server.js -i max",
    "build": "astro build",
    "dev:vercel": "PLATFORM='vercel' npx vercel dev",
    "dev:netlify": "PLATFORM='netlify' npx netlify dev",
    "dev:api": "DISABLE_GUI='true' PORT='3001' nodemon server",
    "dev:astro": "NODE_OPTIONS='--max-old-space-size=4096' astro dev --host",
    "dev": "concurrently -c magenta,cyan -n backend,frontend 'yarn dev:api' 'yarn dev:astro'",
    "clean": "rm -rf node_modules/.vite dist .astro && echo '✅ Cache cleaned'",
    "clean:all": "rm -rf node_modules/.vite dist .astro node_modules && echo '✅ Full clean - run yarn install'",
    "restart": "pkill -f 'astro|vite|node' || true && yarn dev",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/e2e",
    "test:auth": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit/auth",
    "test:admin": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit/admin",
    "test:plugins": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit/plugins",
    "test:real": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration tests/e2e"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/react": "^3.3.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/svelte-fontawesome": "^0.2.2",
